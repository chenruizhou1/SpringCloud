<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.crz.springcloud.mapper.OrderMapper">
    
    <resultMap id="BaseResultMap" type="com.crz.springcloud.entity.Order">
        <id column="id" property="id" />
        <result column="order_no" property="orderNo" />
        <result column="user_id" property="userId" />
        <result column="commodity_code" property="commodityCode" />
        <result column="count" property="count" />
        <result column="money" property="money" />
    </resultMap>

    <insert id="createOrder" parameterType="Order" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO order_tbl VALUES(null, #{order.orderNo}, #{order.userId}, #{order.commodityCode}, #{order.count}, #{order.money});
    </insert>

</mapper>